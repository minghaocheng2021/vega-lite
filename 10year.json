{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Median house price per suburb (2011-2020)",
  "repeat": [
    "2011",
    "2012",
    "2013",
    "2014",
    "2015",
    "2016",
    "2017",
    "2018",
    "2019",
    "2020"
  ],
  "columns": 2,
  "spec": {
    "projection": {
      "type": "equirectangular",
      "center": [144.4, -37.6],
      "scale": 21000
    },
    "width": 400,
    "height": 300,
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/minghaocheng2021/vega-lite/main/vicsuburb.geo.json",
          "format": {"type": "topojson", "feature": "VIC_LOCALITY_POLYGON_SHP"}
        },
        "transform": [
          {
            "calculate": "'Data is not available in ' + datum.properties.NAME",
            "as": "note"
          }
        ],
        "mark": {
          "type": "geoshape",
          "fill": "#ddd",
          "stroke": "white",
          "strokeWidth": 1
        },
        "encoding": {"tooltip": {"field": "note"}}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/minghaocheng2021/vega-lite/main/Suburb_House_final.csv"
        },
        "transform": [
          {
            "lookup": "locality",
            "from": {
              "data": {
                "url": "https://raw.githubusercontent.com/minghaocheng2021/vega-lite/main/vicsuburb.geo.json",
                "format": {
                  "type": "topojson",
                  "feature": "VIC_LOCALITY_POLYGON_SHP"
                }
              },
              "key": "properties.NAME"
            },
            "as": "geo"
          }
        ],
        "mark": {"type": "geoshape", "stroke": "#fff", "strokeWidth": 0.5},
        "encoding": {
          "shape": {"field": "geo", "type": "geojson"},
          "color": {
            "field": {"repeat": "repeat"},
            "type": "quantitative",
            "scale": {"domain": [300000, 1500000], "scheme": "blues"},
            "legend": {"format": ".2s", "title": "Median price"}
          },
          "tooltip": [
            {"field": "locality", "type": "nominal", "title": "Suburb"},
            {
              "field": {"repeat": "repeat"},
              "type": "quantitative",
              "title": "Median Price",
              "format": ","
            }
          ]
        }
      },
      {
        "data": {
          "values": [
            {
              "2011": "Year: 2011",
              "2012": "Year: 2012",
              "2013": "Year: 2013",
              "2014": "Year: 2014",
              "2015": "Year: 2015",
              "2016": "Year: 2016",
              "2017": "Year: 2017",
              "2018": "Year: 2018",
              "2019": "Year: 2019",
              "2020": "Year: 2020"
            }
          ]
        },
        "mark": {
          "type": "text",
          "align": "right",
          "baseline": "bottom",
          "x": "width",
          "y": 0
        },
        "encoding": {"text": {"field": {"repeat": "repeat"}}}
      }
    ]
  },
  "config": {}
}